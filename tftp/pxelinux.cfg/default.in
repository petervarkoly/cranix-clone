default	        menu.c32
KBDMAP		german.kbd
timeout		100
prompt		0
allowoptions	1
noescape	0
ui              gfxboot bootlogo

menu master passwd SHA1PASS
menu title CRANIX #VERSION# PXE Boot Enviroment

LABEL Festplatte
    LOCALBOOT 0

LABEL CloneTool
        kernel clone/linux
        append initrd=clone/initrd install=exec:/bin/start_itool Info=tftp://install/linuxrc.config ITOOL=partimage STARTCMD=clone SLEEP=1 insecure=1 dhcptimeout=120


